// draws the ground and red flowers and grass spots
moveTo(0, 230);
//sets up pen
penDown();
penUp();
// draws ground
drawGround();
drawFlower(40, 375);
drawFlower(69, 420);
drawFlower(120, 410);
drawFlower(195, 430);
drawFlower(281, 392);
drawFlower(225, 370);
function drawGround() {
moveTo(160, 892);
penDown();
penRGB(110, 200, 110);
dot(575);
show();
penUp();
}
// draw flower
function drawFlower(xval, yval) {
moveTo(xval, yval);
penRGB(90, 170, 90);
penDown();
penWidth(3);
arcLeft(randomNumber(10, 60), 25);
penUp();
moveTo(xval, yval);
penDown();
turnTo(0);
penWidth(3);
arcRight(randomNumber(45, 70), 25);
penUp();
moveTo(xval, yval);
penDown();
turnTo(0);
moveForward(25);
penRGB(220, 90, 90);
dot(5);
penUp();
hide();
// draws all parts of tree
// base of tree
drawTrunk();
function drawTrunk() {
penColor("brown");
penUp();
penWidth(25);
moveTo(160,385);
penDown();
moveForward(115);
penUp();
// draws main tree
// sets up pen
moveTo(148,324);
turnLeft(90);
hide();
penColor("green");
penWidth(15);
penDown();
moveForward(50);
// drawing the main tree
show();
turnRight(113);
moveForward(175);
turnRight(136);
moveForward(170);
turnRight(110);
moveForward(54);
hide();
penUp();
}
