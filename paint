// draws blue background
drawBackground();
function drawBackground () {
penColor("LightBlue");
dot(500);
}
// draws yellows sun behind mountains
drawPic();
function drawPic() {
hide();
drawSun();
function drawSun() {
moveTo(195, 140);
penRGB(250, 170, 100, 1);
dot(40);
}
// draws clouds at top of sky
// draws white puffy clouds
drawClouds();
function drawClouds() {
// sets up pen
penUp();
moveTo(0, 77);
penDown();
penRGB(240, 240, 240);
// colors in the white of clouds
for (var i = 0; i < 4; i++) {
turnRight(118);
arcLeft(100, 60);
}
penUp();
moveTo(0, 50);
penDown();
penRGB(240, 240, 240);
for (var i = 0; i < 4; i++) {
turnRight(100);
arcLeft(122, 50);
}
for (var i = 0; i < 50; i++) {
penUp();
moveTo(randomNumber(550, 30), randomNumber(70, 20));
penDown();
penRGB(240, 240, 240, 0.5);
dot(50);
 
}
}
