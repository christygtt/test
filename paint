// draws overlapping mountains
function drawMountain() {
penDown();
penRGB(250, 230, 220, 1);
penWidth(60);
turnRight(50);
moveForward(100);
turnRight(120);
moveForward(100);
}
// draws triangle of snow on top of mountain
function drawSnow() {
penDown();
penRGB(222, 222, 222, 1);
penWidth(17);
turnLeft(120);
moveForward(50);
turnRight(107);
moveForward(53);
turnRight(90);
arcRight(60, 25);
turnLeft(50);
arcRight(60, 35);
}
function drawDot() {
penDown();
penRGB(222, 222, 222, 1);
dot(23);
}
function drawmDot() {
penDown();
penRGB(250, 230, 220, 1);
dot(45);
}
}
